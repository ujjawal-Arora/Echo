{
  "name": "Echoo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "install:all": "npm install && npm run build"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.2.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@types/react": "^18.3.10",
    "autoprefixer": "^10.4.20",
    "lucide-react": "^0.488.0",
    "postcss": "^8.4.47",
    "react-icons": "^5.3.0",
    "tailwindcss": "^3.4.13"
  },
  "resolutions": {
    "rimraf": "^4.0.0",
    "glob": "^9.0.0"
  }
}
